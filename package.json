{
    "name": "super-admin",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "lint-staged": {
        "src/**/*.{ts,tsx,scss}": [
            "prettier --write"
        ],
        "./*.{js,cjs,html,jsx,josn,md,ts,tsx}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "dev": "vite  --host ",
        "format": "prettier --write \"src/**/*.{ts,tsx}\" \"./*.{js,cjs,html,jsx,json,md,ts,tsx}\"",
        "build": "tsc && vite build",
        "build-gh": "cross-env BASE_SITE_URL=/super-admin/ vite build",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prepare": "husky install"
    },
    "dependencies": {
        "@ant-design/cssinjs": "^1.17.2",
        "@ant-design/icons": "^5.2.6",
        "@ant-design/pro-components": "^2.6.35",
        "@emotion/react": "^11.11.1",
        "@reduxjs/toolkit": "^1.9.7",
        "@wangeditor/editor": "^5.1.23",
        "@wangeditor/editor-for-react": "^1.0.6",
        "antd": "^5.11.0",
        "axios": "^1.6.1",
        "color": "^4.2.3",
        "cross-env": "^7.0.3",
        "dayjs": "^1.11.10",
        "keepalive-for-react": "^1.0.18",
        "nookies": "^2.5.2",
        "nprogress": "^0.2.0",
        "qs": "^6.11.2",
        "ramda": "^0.29.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.3",
        "react-router-dom": "^6.18.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@emotion/babel-preset-css-prop": "^11.11.0",
        "@types/node": "^20.9.0",
        "@types/nprogress": "^0.2.3",
        "@types/ramda": "^0.29.8",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vitejs/plugin-react": "^4.1.1",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.53.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "husky": "^8.0.3",
        "lint-staged": "^15.0.2",
        "postcss": "^8.4.31",
        "prettier": "^3.0.3",
        "rollup-plugin-visualizer": "^5.9.2",
        "sass": "^1.69.5",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^4.5.0"
    }
}
